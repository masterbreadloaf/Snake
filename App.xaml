<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="StartMenu.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#FFFF00</SolidColorBrush>
        <SolidColorBrush x:Key="GridgroundColor">#FF00FF</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#4F4867</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#000</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
